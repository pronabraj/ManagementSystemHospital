@model IEnumerable<Hospital.Doctor_Info>








<p>
    
    <div class="container">
    <button type="button" class="btn btn-success" onclick="myFunction()">PDF Download</button>
        </div>

</p>
<script>
    function myFunction() {
        window.print();
    }

</script>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Doctor_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Speacalist)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Duty_time)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Phone_Number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Salary)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Doctor_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Speacalist)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Duty_time)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Phone_Number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Salary)
        </td>
        <td>
         
        </td>
    </tr>
}

</table>
